  <testMethods
      name="CreateAndGetTeacher">
    <statements
        xsi:type="javatest:LineStatement"
        lineContent="HttpClient httpClient = HttpClients.createDefault();"/>
    <statements
        xsi:type="javatest:LineStatement"
        lineContent="var httpRequest = new HttpPost(&quot;https://api.ntnu.no/teachers&quot;);"/>
    <statements
        xsi:type="javatest:LineStatement"
        lineContent="String body = &quot;{&quot;name&quot;: &quot;Leonardo Montecchi&quot;}&quot;;"/>
    <statements
        xsi:type="javatest:LineStatement"
        lineContent="var response = httpClient.execute(httpRequest);"/>
    <statements
        xsi:type="javatest:LineStatement"
        lineContent="assertThat(response.getCode()).isIn(201);"/>
    <statements
        xsi:type="javatest:LineStatement"
        lineContent="&#xA;"/>
    <statements
        xsi:type="javatest:LineStatement"
        lineContent="HttpClient httpClient = HttpClients.createDefault();"/>
    <statements
